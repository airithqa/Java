    public static int [] FillArray(int begin, int end, int step){
        int []arr=new int[1000];
        Random rnd = new Random();
        for (begin=0;begin<end;begin+=step){
            arr[begin]=rnd.nextInt(10)+1;
            System.out.println(arr[begin]);
        }
        return arr;

    }

